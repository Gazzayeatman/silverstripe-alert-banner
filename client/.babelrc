{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    [
      "transform-postcss",
      {
        "config": "./postcss.config.js",
        "extensions": [
          ".css, .scss"
        ]
      }
    ],
    [
      "babel-plugin-transform-class-properties"
    ]
  ]
}
